{
  "name": "vn.agest.demo-wdio",
  "type": "module",
  "devDependencies": {
    "@types/node": "^25.0.10",
    "@wdio/allure-reporter": "^9.23.2",
    "@wdio/cli": "^9.23.2",
    "@wdio/globals": "^9.23.0",
    "@wdio/local-runner": "^9.23.2",
    "@wdio/mocha-framework": "^9.23.2",
    "@wdio/spec-reporter": "^9.23.2",
    "@wdio/visual-service": "^9.1.2",
    "allure-commandline": "^2.36.0",
    "expect-webdriverio": "^5.6.4",
    "rimraf": "^6.1.2",
    "wdio-json-reporter": "^4.0.1"
  },
  "scripts": {
    "start": "wdio run ./wdio.conf.ts",
    "start:report": "wdio run ./wdio.conf.ts && allure generate --clean && allure open",
    "report:gen": "allure generate --clean",
    "report:open": "allure open",
    "report:clear": "rimraf allure-results allure-report",
    "report": "npm run report:gen && npm run report:open"
  }
}
